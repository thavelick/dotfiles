#!/usr/bin/env python3
# From https://codeberg.org/EvanHahn/dotfiles/src/branch/main/home/bin/bin/markdownquote
import sys

for line in sys.stdin:
    trimmed = line.strip()
    if trimmed == "":
        print(">")
    elif trimmed.startswith(">"):
        print(trimmed)
    else:
        print("> " + trimmed)
